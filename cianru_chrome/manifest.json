{
    "manifest_version": 2,

    "name": "Улучшайзер cian.ru",
    "description": "Повышение наглядности результатов поиска сайта cian.ru",
    "version": "1.1.2",

    "content_scripts":
    [
        {
            "matches": ["http://www.cian.ru/*"],
            "js": [
                "vendor/assets/javascripts/jquery-1.10.2.min.js",
                "assets/javascripts/search_results.js"
            ],
            "run_at": "document_end"
        }
    ]
}